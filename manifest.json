{
  "manifest_version": 3,
  "name": "Flow Downloader Auto | Tandev.foto",
  "version": "2.0.0",
  "description": "Premium Video Downloader - High Performance Core.",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Flow Downloader"
  },
  "background": {
    "service_worker": "background.js"
  }
}